//Problema: Imprimir do numero 0 a 1000
programa {
  funcao inicio() {
    
    //Declaração de variaveis
    inteiro Contador

    //Contador de 0 a 1000 para exibir numeros de 0 a 1000
    para(Contador = 0; Contador <= 1000; Contador++){
      escreva(Contador,", ")
    }
  }
}
